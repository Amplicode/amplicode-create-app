{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "dev",
			"isBackground": true,
			"presentation": {
				"reveal": "never",
				"group": "watchers"
			},
			"problemMatcher": {
				"pattern": {
					"regexp": "^(.*)$",
					"file": 1,
					"location": 2,
					"message": 3
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": ".*",
					"endsPattern": ".*"
				}
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
